{
  "name": "mecanica365-backend",
  "version": "0.0.1",
  "description": "Mecânica365 - Backend API para oficinas mecânicas",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0 --cache --cache-location .eslintcache",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0 --cache --cache-location .eslintcache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:safe": "jest --runInBand --no-cache --forceExit --no-coverage || echo 'Alguns testes podem ter falhado devido a erros de I/O com múltiplos projetos rodando'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sonar": "powershell -ExecutionPolicy Bypass -File scripts/sonar-scanner.ps1",
    "sonar:local": "npm run test:cov && npm run sonar",
    "sonar:check": "powershell -ExecutionPolicy Bypass -File scripts/sonar-scanner.ps1 -CheckQualityGate",
    "sonar:with-coverage": "powershell -ExecutionPolicy Bypass -File scripts/sonar-scanner.ps1 -WithCoverage",
    "sonar:fix": "powershell -ExecutionPolicy Bypass -File scripts/fix-sonar-issues.ps1 -Token $env:SONAR_TOKEN -AutoFix",
    "sonar:fix:full": "powershell -ExecutionPolicy Bypass -File scripts/fix-sonar-issues.ps1 -Token $env:SONAR_TOKEN -AutoFix -RunTests",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "docker:restart": "docker-compose restart",
    "docker:ps": "docker-compose ps",
    "docker:prod:up": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:migrate": "docker-compose exec backend npx prisma migrate dev",
    "docker:migrate:deploy": "docker-compose -f docker-compose.prod.yml exec backend npx prisma migrate deploy",
    "docker:prisma:generate": "docker-compose exec backend npx prisma generate",
    "db:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-database.ps1",
    "db:seed": "docker-compose exec backend npx ts-node prisma/seeds/index.ts",
    "db:fix-migration": "powershell -ExecutionPolicy Bypass -File scripts/fix-failed-migration.ps1",
    "db:fix-migration:force": "powershell -ExecutionPolicy Bypass -File scripts/fix-failed-migration.ps1 -Force"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "^6.19.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "exceljs": "^4.4.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.8.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^20.0.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.19.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
